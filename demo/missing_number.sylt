## finds the gap in a series of (almost) consecutive numbers

let numbers = List.range(1 5)
List.del(numbers 3)
printLn(numbers)

let missing = nil
let index = 0

while (missing = nil) do
    let num = numbers[index]
    let next = numbers[index + 1]

    if (next > num + 1) missing <- num + 1
    else index <- incr(index)
end

printLn(missing)

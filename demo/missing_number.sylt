## finds the gap in a series of (almost) consecutive numbers

let numbers = List.range(1 5)
List.del(3 numbers)
printLn(numbers)

let missing = nil
let index = 0

while missing = nil: do
    let num = List.get(index numbers)
    let next = List.get(index + 1 numbers)

    if next > num + 1: missing <- num + 1
    else index += 1
end

printLn(missing)

## if a number is divisible by 3: print "Fizz", if divisible by 5: print "Buzz", if divisible
## by both 3 and 5: print "FizzBuzz", otherwise print the number

for: fun num -> do
    let output = ""
    if num % 3 is 0 then output <- "Fizz"
    if num % 5 is 0 then output += "Buzz"
    if output is "" then output <- num

    printLn: output!
end
1 100!

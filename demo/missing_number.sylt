## finds the gap in a series of (almost) consecutive numbers

let numbers = List.range(1 5)
List.del(numbers 3)
printLn(numbers)

let missing = nil

rep(fun index -> {
    let num = numbers[index]
    let next = numbers[index + 1]

    if (next > num + 1)
        missing <- num + 1
} List.length(numbers) - 1)

printLn(missing)

## this program prints its own source code

let source_code = System.src().text

## split the source into a list of lines so we can add line numbers
let lines = String.split(source_code, "\n")

List.iter(fun(i, code) -> {
	print(String.rightPad(toString(i + 1), 5, " "))
	print(" ")
	printLn(code)
}, lines)

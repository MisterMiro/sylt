let equality _ = do
	## contains a value of each type
	let values = [
		null        ## Null
		true        ## Bool
		235	        ## Number
		[123]	  	## List
        {"a" = "b"} ## Dict
		"'_'"	  	## String
		fun x -> x	## Func
	]
	
	List.iter(fun skipIndex value -> do
		## equal to itself
		ensure(value is value)
		
		## not equal to a value of any other
		## type
		List.iter(fun index other ->
            if index not skipIndex then ensure(value not other)
            values)
	end values)
	
	## perform more type-specific tests
	ensure(true not false)
	
	ensure([] is [])
	ensure([[]] is [[]])
	ensure([0] not [1])
	
	ensure("" is "")
	ensure("ABC" not "abc")
	
	ensure(Math.pow not Math.sqrt)
end


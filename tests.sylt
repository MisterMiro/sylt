	## ...and neither do captured values
	let c = 35
	let result = do
		let v = 9
		let sum = c + v
		123
	end
	ensure(result is 123)
	
	## assigning shadowed variable
	let x = true
	do
		let x = true
		x <- false
		ensure(x is false)
	end
	ensure(x is true)
end


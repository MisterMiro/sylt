## code for checking whether a number is prime

let isPrime n = do
    if n < 2: return false
    let check index = if num is index: true
        else if num % index is 0: false
        else check(index + 1)
    check(2)
end

let numbers = List.range(0..100)
numbers <- List.filter(isPrime numbers)
printLn(numbers)

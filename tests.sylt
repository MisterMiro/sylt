	## a functions stack window
	let outside = "outside"
	let f = fun -> outside
	ensure(f() is outside)
	
	## moving surrounding context to
	## the heap if necessary
	let f = fun -> do
		let keep = "jewel"
		fun -> keep
	end
	ensure(f()() is "jewel")
	
